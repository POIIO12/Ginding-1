#include <string>
#include <ctime>
#include <iostream>
using namespace std;
//amper sign & makes two variables the same,like setting them to equal.
void multiplyBy(int &x, int y){
    x = x*y;
    y = -9999;
    //variables only exists in this scope. Scope of variables is very important.
}
void increaseBy(int &z, int b){
    z += 21;
}
int main()
{
    int x = 5;
    int y = 10;
    multiplyBy(x, y);
    cout << x << y;
    int z = 53
    increaseBy(z, 21);
    cout << z;
    increaseBy(z, 5);// should be 79
    cout << z;
    return 0;
}
